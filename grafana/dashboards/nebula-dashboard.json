{
  "id": null,
  "uid": "nebula-main",
  "title": "Nebula Backend Observability",
  "tags": ["nebula", "backend", "axum"],
  "timezone": "",
  "schemaVersion": 38,
  "version": 3,
  "refresh": "10s",
  "editable": true,
  "graphTooltip": 0,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["5m", "15m", "30m", "1h", "6h", "12h", "24h"]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Global p95 Latency (all endpoints)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(axum_http_requests_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 latency"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "p95 Latency per Endpoint",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(axum_http_requests_duration_seconds_bucket[5m])))",
          "legendFormat": "{{endpoint}}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Requests per Second (RPS) per Endpoint",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (endpoint, method) (rate(axum_http_requests_total[1m]))",
          "legendFormat": "{{method}} {{endpoint}}"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Error Rate (5xx as % of all requests)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 8, "w": 8, "x": 12, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (sum(rate(axum_http_requests_total{status=~\"5..\"}[5m])) / sum(rate(axum_http_requests_total[5m])))",
          "legendFormat": "5xx %"
        }
      ]
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Total In-flight Requests",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 8, "w": 4, "x": 20, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(axum_http_requests_pending)",
          "legendFormat": "in-flight"
        }
      ]
    },
    {
      "id": 6,
      "type": "bargauge",
      "title": "Status Code Breakdown (rate over 5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status) (rate(axum_http_requests_total[5m]))",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Per-Endpoint Detail: p95, AVG & RPS",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-ds"
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 24 },
      "repeat": "endpoint",
      "repeatDirection": "v",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 6
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(axum_http_requests_duration_seconds_bucket{endpoint=~\"$endpoint\"}[5m])))",
          "legendFormat": "p95"
        },
        {
          "refId": "B",
          "expr": "sum(rate(axum_http_requests_duration_seconds_sum{endpoint=~\"$endpoint\"}[5m])) / sum(rate(axum_http_requests_duration_seconds_count{endpoint=~\"$endpoint\"}[5m]))",
          "legendFormat": "avg"
        },
        {
          "refId": "C",
          "expr": "sum by (method) (rate(axum_http_requests_total{endpoint=~\"$endpoint\"}[1m]))",
          "legendFormat": "RPS {{method}}"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "endpoint",
        "label": "Endpoint",
        "type": "query",
        "hide": 0,
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus-ds"
        },
        "query": "label_values(axum_http_requests_total, endpoint)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": []
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)"
      }
    ]
  }
}

